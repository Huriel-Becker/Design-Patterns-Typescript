{
    "compilerOptions": {
        "target": "ES2019",
        "module": "commonjs",
        "lib": ["DOM","ESNext"],
        "outDir": "./dist",
        "rootDir": "./src",
        "removeComments": true,
        "noEmitOnError": true,
        "strict": true,
        "esModuleInterop": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true,
    },
    "include": [
        "./src"
    ]
}